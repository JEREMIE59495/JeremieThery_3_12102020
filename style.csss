/*============ menu==========*/
/* animation bouton menu*/
@keyframes btn_entrees {
  0% {
    transform-origin: 50% bottom;
    transform: scaleY(0);
    opacity: 0;
  }
  100% {
    transform-origin: 50% bottom;
    transform: scaleY(1);
    opacity: 1;
  }
}
@keyframes btn_plats {
  0% {
    transform-origin: 50% bottom;
    transform: scaleY(0);
    opacity: 0;
  }
  20% {
    transform-origin: 50% bottom;
    transform: scaleY(0);
    opacity: 0;
  }
  100% {
    transform-origin: 50% bottom;
    transform: scaleY(1);
    opacity: 1;
  }
}
@keyframes btn_desserts {
  0% {
    transform-origin: 50% bottom;
    transform: scaleY(0);
    opacity: 0;
  }
  20% {
    transform-origin: 50% bottom;
    transform: scaleY(0);
    opacity: 0;
  }
  100% {
    transform-origin: 50% bottom;
    transform: scaleY(1);
    opacity: 1;
  }
}
