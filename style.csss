/*  animation validation menu*/
.btn_entrees:active .prix span, .btn_plats:active .prix span, .btn_desserts:active .prix span, .X_btn_plats:active .prix span {
  transform: translate(-3em, 0) rotate(360deg);
}
.btn_entrees:active .X_prix .big-bloc, .btn_plats:active .X_prix .big-bloc, .btn_desserts:active .X_prix .big-bloc, .X_btn_plats:active .X_prix .big-bloc {
  transform: translate(-1.5em, 0) rotate(360deg);
}
.btn_entrees:active .validation, .btn_plats:active .validation, .btn_desserts:active .validation, .X_btn_plats:active .validation {
  transform: translate(-3em, 0);
}
.btn_entrees:active .X_validation, .btn_plats:active .X_validation, .btn_desserts:active .X_validation, .X_btn_plats:active .X_validation {
  transform: translate(-3em, 0);
}
.btn_entrees:active .intituler, .btn_plats:active .intituler, .btn_desserts:active .intituler, .X_btn_plats:active .intituler {
  width: 60%;
}

.validation, .X_validation {
  transform-origin: 100%, 0;
  transition: transform 400ms ease-in-out;
}

