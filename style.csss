/*============ mode responsive ==============*/
body {
  width: auto;
  text-align: center;
  margin: 0em;
  position: absolute;
  z-index: 3;
}

/*loading spinner*/
.font-loading {
  width: 100%;
  height: 4em;
  background: #a7a6a6;
  animation: font-loading 5000ms;
  position: absolute;
  z-index: 0;
  opacity: 0;
}

.load .fas {
  margin-top: 0.5em;
  font-size: 2em;
  transform: rotate(0deg);
}

.load .fas {
  transform: rotate(-2160deg);
  transition: transform 5000ms ease-in-out;
}

.load {
  width: 9em;
  height: 9em;
  margin-top: 28%;
  margin-left: 35%;
  animation: load 5000ms;
  opacity: 0;
  position: absolute;
  z-index: 0;
}
.load__circle--0 {
  width: 9em;
  height: 9em;
  background: #9356DC;
  opacity: 0.5;
  border-radius: 50%;
  position: absolute;
  animation: circle 3000ms backwards;
}
.load__circle--1 {
  width: 7.7em;
  height: 7.7em;
  background: #a7a6a6;
  border-radius: 50%;
  position: absolute;
  margin-left: 10px;
  margin-top: 10px;
  animation: circle 3000ms backwards;
}
.load__circle--2 {
  width: 6.5em;
  height: 6.5em;
  border-radius: 50%;
  background: #FF79DA;
  opacity: 0.5;
  margin-top: 20px;
  margin-left: 20px;
  position: absolute;
  animation: circle 3000ms backwards;
}
.load__circle--3 {
  width: 5.2em;
  height: 5.2em;
  border-radius: 50%;
  margin-top: 0em;
  background: #a7a6a6;
  position: absolute;
  margin-left: 30px;
  margin-top: 30px;
  animation: circle 3000ms backwards;
}
.load__circle--4 {
  width: 4em;
  height: 4em;
  background: #99E2D0;
  opacity: 0.5;
  border-radius: 50%;
  margin-top: 40px;
  margin-left: 40px;
  position: absolute;
  animation: circle 3000ms backwards;
}

.load__circle--0 {
  animation-delay: 1600ms;
}

.load__circle--1 {
  animation-delay: 1200ms;
}

.load__circle--2 {
  animation-delay: 400ms;
}

.load__circle--3 {
  animation-delay: 800ms;
}

.load__circle--4 {
  animation-delay: 0ms;
}

@keyframes circle {
  0% {
    transform: scale(0.1);
    opacity: 0;
  }
  100% {
    transform: scale(1);
    opacity: 1;
  }
}
@keyframes load {
  0% {
    transform: scale(1);
    opacity: 0.9;
    margin-top: 50%;
  }
  50% {
    transform: scale(1);
    opacity: 0.9;
    margin-top: 50%;
  }
  80% {
    transform: scale(1);
    opacity: 0.9;
    margin-top: 50%;
  }
  95% {
    transform: scale(1);
    opacity: 0.9;
    margin-top: 50%;
  }
  100% {
    transform: scale(0);
    opacity: 0;
    margin-top: 50%;
  }
}
@keyframes font-loading {
  0% {
    transform: scaleY(65);
    opacity: 0.9;
  }
  98% {
    transform: scaleY(65);
    opacity: 0.9;
  }
  100% {
    transform: scaleY(0);
    opacity: 0;
  }
}
